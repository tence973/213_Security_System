const int buzzer = 32; // GPIO pin for active buzzer
void setup() {
  pinMode(buzzer,OUTPUT); //initialize the buzzer pin as an output

}

void loop() {
   unsigned char i;
   for(i=0;i<80;i++)
    {
      digitalWrite(buzzer,HIGH);
      delay(1000);//wait for 1s
      digitalWrite(buzzer,LOW);
      delay(600);//wait for 600ms
    }

}
